{
    "name": "laravie/codex-security",
    "description": "Security Component for Laravie Codex",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Codex\\Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Codex\\Security\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "nesbot/carbon": "^2.27"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.10.5",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev"
}
